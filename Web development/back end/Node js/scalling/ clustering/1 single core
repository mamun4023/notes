const express = require('express')

const app = express();
app.get('/', async(req, res)=>{
    let result = 0;

    for(let i = 0; i<2000000000; i++){
        result+=i
    }

    return res.json({"processID" : process.pid, result })
    // console.log(result)
})

app.listen(3000, ()=>{
    console.log(`PORT 3000 PID  ${process.pid}`)
})
