
// interface allow duplicated 
// interface only use for object
// interface does not allow primitive type like
// interface id = string    
interface Book{
    title : string,
    writter : string
}

interface Book{
    price : number
}

const book: Book = {
    title : 'The Habits',
    writter : 'H M Akd',
    price : 10
   
}

// type does not allow to duplicated 
// it is use for object and primitieve data like 
// type allow to define primitive type
//type id = string;


type Book{
    title : string,
    writter : string
}

type Book{
    price : number
}


